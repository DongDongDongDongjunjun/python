import random
while(1) :
    com = random.randint(1, 2)
    user = int(input('odd : 1, even : 2|n' ))

    print('COM(%d) : USER(%d)' %(com, user))
    if com == user :
        print('Correct')
    else :
        print('Incorrect')









from random import*
while(1):
    com = randint(1,3)
    print("1 : 가위, 2 : 바위, 3: 보")
    user = int(input())
    
    if com == 1:
        com = "가위"
    elif com == 2:
        com = "바위"
    else :
        com = "보"

    if user == 1:
        
        user = "가위"
    elif user == 2:
        user = "바위"
    else :
        user = "보"

    print("Com(%s) : User(%s)" %(com,user))


    if com == user :
        print("Draw")
    elif com == "가위":
        if user == "바위":
            print("User Wins!")
        elif user == "보":
            print("Com Wins!")

    elif com == "바위":
        if user == "가위":
            print("Com Wins!")
        if user == "보":
            print("User Wins!")

    elif com == "보":
        if user == "가위":
            print("User Wins!")
        if user == "바위":
            print("Com wins!")        

        













