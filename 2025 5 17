    print(i, end= ' ')




n = int(input())
for i in range(1, n+1) :
    print(i, end = ' ')







for i in range(1,101) :
    if i % 2 == 0 :
        print(i, end = ' ')
    





sum = 0
h_list = [159, 163, 173, 158, 169]

for h in h_list :
    sum += h

print("sum : %d" % sum)
print("avg : %.2f" % (sum/len(h_list)))






sum = 0
for i in range(5):
    n = int(input("키 입력 : "))
    sum += n
    
print("sum : %d" % sum)
print("avg : %.2f" %(sum/5))




for i in range (3) :
    for j in range(1, 6) :
        print(j, end = ' ')
    print()
        






for i in range(2, 10) :
    print("< %d 단 >" % i)
    for j in range(1, 10) :
        print("%d * %d = %2d" % (i, j, i*j))
    print()







sum = 0
n = int(input())
for i in range(1,n+1):
    if i %3 == 0:
        sum += i
print(sum)

        




























min = 10000000000000000000000000000000000
for i in range(5):
    n = int(input())
    if n < min :
        min = n
print(min)










n = int(input())
i = 1
while i <=n :
    if i%3== 0:
        sum += i
    i += 1
    


    
