sum = 0
i = 1
while i <= 100:
    sum += i
    i = i + 1
    print(sum, end = ' ')
    
    
List = ['a', 'b', 'c']
for s in List :
    print(s)
num_list = [1, 2, 3, 4, 5]
sum = 0
for num in num_list :
    sum += num
print('avg : %d' % (sum//5))
aList = [1, 2, 3]
bList = [10, 100, 1000]
for a in aList :
    for b in bList :
          print(a * b, end = ' ')
    print()

